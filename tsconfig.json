{
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "jsx": "react",
    "strict": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "baseUrl": "./src",
    "paths": {
      "@/*": [
        "*"
      ]
    },
    "typeRoots": ["./src/types"],
  },
  "exclude": [
    "node_modules"
  ],
  "include": [
    "src",
    "env.d.ts"
, "babel.config.js"  ],
  "extends": "expo/tsconfig.base"
}
